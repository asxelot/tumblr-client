{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "react-native"
  ],

  "rules": {
    "semi": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "react/sort-comp": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0
  }
}
